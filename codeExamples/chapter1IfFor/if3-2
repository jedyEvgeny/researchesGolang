package main

import "fmt"

func main(){
    var num uint
    fmt.Scan(&num)
    if num <=9 {
        fmt.Println(num)
    } else if num >=10 && num <=99 {
        fmt.Println(num/10)
    } else if num >=100 && num <=999 {
        fmt.Println(num/100)
    } else if num >=1000 && num <=9999 {    
        fmt.Println(num/1000)
    } else {fmt.Println(num/10000)}    
 
}
